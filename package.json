    "url": "git://github.com/balderdashy/sails-postgresql.git"
  },
  "dependencies": {
    "pg.js": "~3.3.0",
    "lodash": "~2.4.1",
    "async": "~0.9.0",
    "waterline-errors": "~0.10.0",
    "waterline-sequel": "git+https://github.com/jfgodoy/waterline-sequel.git#geomv0.0.17",
